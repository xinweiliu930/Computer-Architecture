.ORIG x3000
AND R0, R0 #0
ADD R0, R0 #4
LEA R3,A
LDW R3,R3 #0
LEA R4,B
LDW R4,R4, #0
STW R4,R3, #0
LEA R5,C
LDW R5,R5, #0
STW R5,R3, #1
LEA R6,D
LDW R6,R6, #0
STW R6,R3, #2
LEA R7,E
LDW R7,R7, #0
STW R7,R3, #3
GO LDB R1,R3,#0
LDB R2,R3,#1
STB R1,R3,#1
STB R2,R3,#0
ADD R3,R3,#2
ADD R0,R0,#-1
BRp GO	
AND R0,R1,x0050
HALT
A .FILL x3050
B .FILL x0001
C .fill x100A
D .fill x0045
E .fill xE050	
.END

